{
  "name": "ew-demo-node-cli",
  "version": "1.0.0",
  "description": "An interactive CLI tool for Embedded Wallets",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev src/app.ts",
    "prod": "node dist/src/app.js",
    "start": "ts-node src/app.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fireblocks/embedded-wallet-sdk": "^0.0.24",
    "@fireblocks/ncw-js-sdk": "^12.5.5",
    "chalk": "4.1.2",
    "dotenv": "^16.4.7",
    "firebase-admin": "^13.1.0",
    "inquirer": "^12.4.2",
    "inquirer-search-list": "^1.2.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@fireblocks/ts-sdk": "^8.1.0",
    "@types/node": "^22.15.18",
    "eslint": "^9.27.0",
    "fireblocks-sdk": "^5.36.0",
    "husky": "^9.0.11",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "keywords": [
    "cli",
    "tool",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}
